            <div class="col-sm-4">
              <div class="icon-big text-center">
                <img src="/assets/img/6.jpg" />
              </div>
            </div>
            <div class="col-sm-8">
              <div class="detail">
                <p class="detail-subtitle">Lifting</p>
                <span class="number">55</span>
